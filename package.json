{
  "name": "oxps-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel src/index.html",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --out-dir out/js --out-file graphiql.tpl --no-cache --public-url /modules/oxps/graphql/out/js"
  },
  "dependencies": {
    "autoprefixer": "^7.2.1",
    "babel-preset-react-app": "^3.1.0",
    "graphiql": "^0.12.0",
    "graphql": "^14.0.2",
    "isomorphic-fetch": "^2.2.1",
    "parcel-bundler": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "cross-env": "^5.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "http://getnow.local/"
}
